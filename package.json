{
  "name": "@pkkummermo/pkk-scripts",
  "version": "0.1.11",
  "description": "Boostrapper for projects",
  "main": "dist/index.js",
  "bin": {
    "pkk-scripts": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "rimraf dist",
    "build": "tsc && npm run copy:configs",
    "copy:configs": "cp -r scripts/linters/configs dist/scripts/linters/. && cp -r scripts/testers/configs dist/scripts/testers/. && cp -r scripts/formatters/configs dist/scripts/formatters/.",
    "start": "ts-node index.ts --",
    "test": "npm run test:lint",
    "test:lint": "ts-node index.ts lint --include-lint ts"
  },
  "author": "Per Kristian Kummermo",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/cross-spawn": "^6.0.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.13",
    "@types/which": "^1.3.1",
    "ts-node": "^8.1.0"
  },
  "dependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@telenorfrontend/eslint-config-telenor": "^0.1.3",
    "@telenorfrontend/tslint-config-telenor": "^0.1.1",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cross-spawn": "^6.0.5",
    "eslint": "^5.16.0",
    "glob": "^7.1.4",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-junit-reporter": "^1.1.0",
    "lint-staged": "^8.1.7",
    "ora": "^3.4.0",
    "prettier": "^1.17.1",
    "read-pkg-up": "^6.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5",
    "which": "^1.3.1"
  }
}
